<nav>
  <%= link_to 'Films Index', '/films' %>
  <%= link_to 'Directors Index', '/directors' %>
</nav>

<h1>Directors Index</h1>
<h2><%= link_to 'Add New Director', '/directors/new' %></h2>
<br>
<% @directors.desc_order_by_created.each do |director| %>
  <p id="<%= director.id %>"><%= director.name %> -
  <%= link_to 'Edit Director', "/directors/#{director.id}/edit" %> -
  Created at: <%= director.created_at %></p>
<% end %>